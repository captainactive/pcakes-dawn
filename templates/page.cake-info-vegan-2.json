    "9629d87c-f498-4fcc-89a0-54e3c68a1ab9": {
      "type": "custom-liquid",
      "settings": {
        "custom_liquid": "<div class=\"page-width info-pages\">\t\n\t<img loading=\"lazy\" style=\"width: 330px;\" class=\"imgcertdel4\" alt=\"Certified Delicious\" src=\"https://cdn.shopify.com/s/files/1/0024/7441/0043/files/certified-delicious-logo-280.png?v=1678148774\" />\n\t<h2>Custom Cake Icing</h2>\n\t<p>We ice our cakes in a light, airy icing, instead of hard fondant or overly sugary buttercream. Our light icing provides the perfect balance for flavor and texture as a topper and filling to our cakes.</p>\n\n\n\n</div>",
        "color_scheme": "background-1",
        "padding_top": 40,
        "padding_bottom": 52
      }
