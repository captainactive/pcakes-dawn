    "af2414ba-9c6a-4d1d-820d-0acb3e435447": {
      "type": "custom-liquid",
      "settings": {
        "custom_liquid": "<div class=\"page-width info-pages\">\t\n\n\n\t<h2>Custom Cakes Explained</h2>\n\t\n\t<p>New to cake lingo? We are here to help. Let's get visual.</p>\n<br><br> \n\t<div class=\"layer-cont-flavor\">\n\t\t<div class=\"layer-sub\">\n            <div class=\"layer-txt1\">Single Layer Cake</div>\n            <div class=\"layer-txt2\">One layer of cake, cut in half and filled.</div>\n\t\t\t<img class=\"layer-img1\" loading=\"lazy\" src=\"https://cdn.shopify.com/s/files/1/0024/7441/0043/files/cake-layer-1.svg?v=1710531886\" alt=\"Cut 2 inches over\" border=\"0\" />\n            <div class=\"layer-txt2\">Typically round and sheet cakes.</div>\n          <div>&nbsp;</div>\n\t\t</div>\t\n\t\t<div class=\"layer-sub\">\n            <div class=\"layer-txt1\">Double Layer Cake</div>\n            <div class=\"layer-txt2\">Two layers of cake, with a filling in the middle.</div>\n\t\t\t<img class=\"layer-img1\" loading=\"lazy\" src=\"https://cdn.shopify.com/s/files/1/0024/7441/0043/files/cake-layer-2.svg?v=1710531886\" alt=\"Cut 2 inches over\" border=\"0\" />\n            <div class=\"layer-txt2\">Typically round and kitchen cakes.</div>\n             <div>&nbsp;</div>\n\t\t</div>\t\n\t\t<div class=\"layer-sub\">\n            <div class=\"layer-txt1\">Tiered Cakes</div>\n            <div class=\"layer-txt2\">Double layer cakes, of different sizes, stacked on top of each other.</div>\n\t\t\t<img class=\"layer-img1\" loading=\"lazy\" src=\"https://cdn.shopify.com/s/files/1/0024/7441/0043/files/cake-layer-3.svg?v=1710531886\" alt=\"Cut 2 inches over\" border=\"0\" />\n            <div class=\"layer-txt2\">Round and square cakes.</div>\n            <div>&nbsp;</div>\n\t\t</div>\t\n    </div>\n<p class=\"tinytext\">The dotted pink line is the filling</p>\n</div>",
        "color_scheme": "background-1",
        "padding_top": 40,
        "padding_bottom": 40
      }
    },
    "3ff3778f-e9ed-4269-85c6-e3d4407a7fbf": {
