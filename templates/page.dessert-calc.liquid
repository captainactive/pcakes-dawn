	}

	.dessertpricerow input:last-child, .dessertpricerow input {
		width: inherit;
		text-align: right;
		padding-right: 0;
	}
	
	.sliderheight {
		display: grid;
		grid-template-columns: 100px 1fr;
		grid-template-rows: 1fr;
	}
	
	.desstotalrow {
		display: grid;
		grid-template-columns: 130px auto;
	}
	.desstotalrow input {
		width: 100px;
		display: grid;
	}
	
	.desstotalrow label {
